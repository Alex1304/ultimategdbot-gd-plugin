<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.github.alex1304.ultimategdbot.gdplugin.database.GDLinkedUsers" table="gd_linked_users">
		<cache usage="read-write" />
		<id name="discordUserId" type="long" column="discord_user_id">
			<generator class="assigned" />
		</id>
		<property name="gdAccountId" type="long" column="gd_account_id" />
		<property name="isLinkActivated" type="boolean" column="is_link_activated" />
		<property name="confirmationToken" type="string" column="confirmation_token" />
	</class>
</hibernate-mapping>