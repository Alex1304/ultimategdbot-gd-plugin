<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.github.alex1304.ultimategdbot.gdplugin.database.GDLeaderboardBans" table="gd_leaderboard_bans">
		<cache usage="read-write" />
		<id name="accountId" type="long" column="account_id">
			<generator class="assigned" />
		</id>
		<property name="bannedBy" type="long" column="banned_by" />
	</class>
</hibernate-mapping>