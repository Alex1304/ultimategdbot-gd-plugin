<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
	<class name="com.github.alex1304.ultimategdbot.gdplugin.database.GDLevelRequestReviews" table="gd_level_request_reviews">
		<cache usage="read-write" />
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		<property name="reviewerId" type="long" column="reviewer_id" />
		<property name="reviewTimestamp" type="timestamp" column="review_timestamp" />
		<property name="reviewContent" type="text" column="review_content" />
		<many-to-one name="submission" class="com.github.alex1304.ultimategdbot.gdplugin.database.GDLevelRequestSubmissions"
				column="submission_id" cascade="all" />
	</class>
</hibernate-mapping>